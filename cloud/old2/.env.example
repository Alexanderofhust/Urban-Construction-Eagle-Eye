# 文件系统数据处理版本的配置文件

# 高德地图API配置
AMAP_API_KEY=your_amap_api_key_here
AMAP_SECURITY_CODE=your_amap_security_code_here

# Google Gemini API配置
GEMINI_API_KEY=your_gemini_api_key_here

# 服务器配置
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
DEBUG=false

# 数据目录配置
DATA_DIR=./data
IMAGE_DIR=./data/images
MASK_DIR=./data/masks
POINTCLOUD_DIR=./data/pointclouds
METADATA_DIR=./data/metadata

# 处理配置
POINTCLOUD_DOWNSAMPLE_RATIO=0.1
MAX_POINTCLOUD_SIZE=1000000
REVIEW_IMAGE_COUNT=10
REVIEW_IMAGE_SIZE=800

# 文件监控配置
MONITOR_INTERVAL=2.0
MAX_FILE_AGE_HOURS=24
AUTO_CLEANUP_ENABLED=true

# AI分析配置
AI_ANALYSIS_TIMEOUT=60
AI_RETRY_COUNT=3
AI_MAX_IMAGES_PER_REQUEST=10

# 缓存配置
CACHE_DIR=./cache
CACHE_MAX_SIZE_MB=1000
CACHE_TTL_HOURS=24

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=./logs/eagle_eye.log
LOG_MAX_SIZE_MB=10
LOG_BACKUP_COUNT=5

# 安全配置
ALLOWED_ORIGINS=*
ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
ALLOWED_HEADERS=*

# 性能配置
MAX_CONCURRENT_SESSIONS=10
MAX_PROCESSING_QUEUE_SIZE=100
BACKGROUND_TASK_TIMEOUT=300
